{
  "name": "enframe",
  "version": "0.0.11",
  "bin": "./bin/enframe.js",
  "description": "Enframe facilitates rapid application creation and continued maintainance.",
  "repository": "git@github.com:aegatlin/binlink.git",
  "author": "Austin Gatlin <austin@gatlin.io>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "lint": "eslint --ext .ts,.tsx srcapp/ srcbin/",
    "bundle": "parcel build -t node -d bin srcbin/enframe.ts",
    "addEnv": "echo '#! /usr/bin/env node\n' | cat - bin/enframe.js > temp && mv temp bin/enframe.js",
    "makeExec": "chmod 755 bin/enframe.js",
    "compile": "yarn bundle && yarn addEnv && yarn makeExec",
    "c": "yarn compile",
    "watch": "onchange -ik 'srcbin/**' -- yarn compile",
    "git-reset": "./git-reset.sh",
    "heroku-reset": "./heroku-reset.sh",
    "test-app-reset": "yarn git-reset && yarn heroku-reset",
    "tar": "yarn test-app-reset",
    "test-app-link": "cd enframe-test-app && yarn && yarn link enframe",
    "test-app": "cd enframe-test-app && yarn enframe",
    "tap": "yarn test-app",
    "test-app-full": "yarn test-app-reset && yarn test-app-link && cd enframe-test-app && yarn enframe",
    "taf": "yarn test-app-full"
  },
  "devDependencies": {
    "@types/chai": "^4.2.0",
    "@types/chai-enzyme": "^0.6.7",
    "@types/compression": "^1.0.1",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/express": "^4.17.1",
    "@types/fs-extra": "^8.0.0",
    "@types/helmet": "^0.0.44",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.2",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "chai": "^4.2.0",
    "chai-enzyme": "^1.0.0-beta.1",
    "compression": "^1.7.4",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "helmet": "^3.20.0",
    "import-fresh": "^3.1.0",
    "mocha": "^6.2.0",
    "onchange": "^6.0.0",
    "parcel": "^1.12.3",
    "prettier": "^1.18.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  }
}
